{
    "version": 2,
    "builds": [
        {
            "src": "main.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["public/**"]
            }
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/main.js"
        }
    ]
}
